<AnchorPane id="AnchorPane" prefHeight="694.0" prefWidth="965.0" styleClass="mainFxmlClass" stylesheets="@signup.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crictrack.SignUpController">
